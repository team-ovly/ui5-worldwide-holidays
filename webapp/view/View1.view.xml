<mvc:View controllerName="ovly.holidays.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:core="sap.ui.core"
	xmlns:form="sap.ui.layout.form" xmlns:layout="sap.ui.layout" xmlns:commons="sap.suite.ui.commons" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" busyIndicatorDelay="0" busy="false" title="Worldwide Holidays">
					<content>
						<form:SimpleForm layout="ResponsiveGridLayout" adjustLabelSpan="true" editable="true" labelSpanL="3" labelSpanM="3" emptySpanL="3" emptySpanM="3">
							<form:content>
								<Label text="Contry" />
								<Select id="select" forceSelection="false">
									<items>
									</items>
								</Select>
								<Label text="Year"/>
								<DatePicker id="datepicker" placeholder="Year" displayFormat="yyyy"/>
								<Label/>
								<Button text="Search" icon="sap-icon://search" press="onSearch"/>
							</form:content>
						</form:SimpleForm>
						<commons:Timeline id="timeline" showHeaderBar="false" alignment="Left">
							<commons:content>
								
							</commons:content>
						</commons:Timeline>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>